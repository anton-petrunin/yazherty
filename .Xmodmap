!    YAZHERTY
!    Anglo-Russkaya foneticheskaya raskladka 
!    Pereklyuchateli: Alt_R i Shift+Cntrl_L
!
     keysym  grave        = grave  asciitilde  Cyrillic_io Cyrillic_IO 
     keysym 1             = 1 exclam  1 exclam
     keysym 2             = 2 at 2 at
     keysym 3             = 3 numbersign 3 numerosign
     keysym 4             = 4 dollar 4 section
     keysym 5             = 5 percent 5 percent
     keysym 6             = 6 asciicircum 6 asciicircum
     keysym 7             = 7 ampersand 7 ampersand
     keysym 8             = 8 asterisk 8 asterisk
     keysym 9             = 9 parenleft 9 parenleft
     keysym 0             = 0 parenright 0 parenright
     keysym equal         = equal plus Cyrillic_yu Cyrillic_YU
     keysym minus         = minus underscore minus Cyrillic_E
!
     keysym  q            =q      Q       Cyrillic_ya Cyrillic_YA           
     keysym  w            =w      W       Cyrillic_zhe Cyrillic_ZHE           
     keysym  e            =e      E       Cyrillic_ie Cyrillic_IE           
     keysym  r            =r      R       Cyrillic_er Cyrillic_ER           
     keysym  t            =t      T       Cyrillic_te Cyrillic_TE           
     keysym  y            =y      Y       Cyrillic_yeru Cyrillic_YERU           
     keysym  u            =u      U       Cyrillic_u Cyrillic_U           
     keysym  i            =i      I       Cyrillic_i Cyrillic_I           
     keysym  o            =o      O       Cyrillic_o Cyrillic_O           
     keysym  p            =p      P       Cyrillic_pe Cyrillic_PE           
!    keysym  bracketleft  =bracketleft braceleft Cyrillic_sha Cyrillic_SHA
     keycode  34 = bracketleft braceleft Cyrillic_sha Cyrillic_SHA
!    keysym  bracketright =bracketright braceright Cyrillic_shcha Cyrillic_SHCHA
     keycode  35 = bracketright braceright Cyrillic_shcha Cyrillic_SHCHA
!    keysym  backslash    =backslash bar   Cyrillic_tse Cyrillic_TSE
     keycode  51 = backslash bar Cyrillic_tse Cyrillic_TSE
!
     keysym  a            =a      A        Cyrillic_a Cyrillic_A           
     keysym  s            =s      S        Cyrillic_es Cyrillic_ES           
     keysym  d            =d      D        Cyrillic_de Cyrillic_DE           
     keysym  f            =f      F        Cyrillic_ef Cyrillic_EF           
     keysym  g            =g      G        Cyrillic_ghe Cyrillic_GHE           
     keysym  h            =h      H        Cyrillic_ha Cyrillic_HA           
     keysym  j            =j      J        Cyrillic_shorti Cyrillic_SHORTI           
     keysym  k            =k      K        Cyrillic_ka Cyrillic_KA           
     keysym  l            =l      L        Cyrillic_el Cyrillic_EL           
     keysym  semicolon    = semicolon colon semicolon colon     
     keysym  apostrophe   =apostrophe quotedbl Cyrillic_softsign Cyrillic_SOFTSIGN
!
     keysym  z            =z      Z        Cyrillic_ze Cyrillic_ZE         
     keysym  x            =x      X        Cyrillic_hardsign Cyrillic_HARDSIGN         
     keysym  c            =c      C        Cyrillic_che Cyrillic_CHE         
     keysym  v            =v      V        Cyrillic_ve Cyrillic_VE         
     keysym  b            =b      B        Cyrillic_be Cyrillic_BE         
     keysym  n            =n      N        Cyrillic_en Cyrillic_EN         
     keysym  m            =m      M        Cyrillic_em Cyrillic_EM         
     keysym  comma        =comma  less     comma guillemotleft         
     keysym  period       =period greater  period guillemotright 
     keysym  slash        = slash question Cyrillic_e question

     keysym  Control_L    =Control_L ISO_Next_Group
     
     keycode 133 = Mode_switch Mode_switch Mode_switch Mode_switch
